	    <div class="pratica" >
	      <table width="100%"  border="0" cellspacing="1">
              <tr>
                <th colspan="2" width="100%" align="left"><span class="pratica-tit">%[NOME]%</span></th>
              </tr>
              <tr>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[LOGIN-TITLE]%: <strong>%[LOGIN]%</strong></td>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[PASSWORD-TITLE]%: <span class="pratica-resalt-01"><strong>********</strong></span></td>
              </tr>
              <tr>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[MANSIONE-TITLE]%: <strong>%[MANSIONE]%</strong></td>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[ONORARIO-TITLE]%: <strong>%[ONORARIO]%</strong></td>
              </tr>
              <tr>
                <td width="100%" colspan="2" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[ID-TITLE]%: <strong>%[ID]%</strong></td>
              </tr>

             </table>
	      <p align="center">%[BUTTON1-TITLE]% | %[BUTTON2-TITLE]%</p>
	    </div>
