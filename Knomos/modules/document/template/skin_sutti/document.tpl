	    <div class="pratica" >
	      <table width="100%"  border="0" cellspacing="1">
              <tr>
                <th width="100%" colspan="2" align="left"><span class="pratica-tit">%[VERSION-TITLE]% %[VERSION]%</span></th>
		         </tr>
              <tr>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'" colspan="2">%[DESCR-TITLE]%: <span class="pratica-resalt-01">%[DESCR]%</span></td>
              </tr>
              <tr>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[OPERATORE-TITLE]%: <strong>%[OPERATORE]%</strong></td>
