	    <div class="pratica" >
	      <table width="100%"  border="0" cellspacing="1">
              <tr>
                <th width="100%"  colspan="2" align="left"><span class="pratica-tit">%[TIT-TITLE]%</span></th>
              </tr>
              <tr>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[NAME-TITLE]%: <strong>%[NAME]%</strong></td>
                <td width="50%" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[DESC-TITLE]%: <span class="pratica-resalt-01"><strong>%[DESC]%</strong></span></td>
              </tr>
              <tr>
                <td width="100%" colspan="2" onMouseOver="this.className='pratica-over-sub'" onMouseOut="this.className='null'">%[ID-TITLE]%: <strong>%[ID]%</strong></td>
              </tr>
              </table>
	      <p align="center">%[BUTTON1-TITLE]% | %[BUTTON2-TITLE]%</p>
	    </div>